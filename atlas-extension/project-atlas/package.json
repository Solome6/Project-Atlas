{
    "name": "project-atlas",
    "displayName": "Project Atlas",
    "description": "Codebase visualizer",
    "version": "0.0.1",
    "publisher": "atlasvisualizer",
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Visualization"
    ],
    "activationEvents": [
        "onCommand:project-atlas.helloWorld",
        "onCommand:project-atlas.openAtlas"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "project-atlas.helloWorld",
                "title": "Hello World"
            },
            {
                "command": "project-atlas.openAtlas",
                "title": "Project Atlas: Open Atlas"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "npm-run-all compile:*",
        "watch": "concurrently npm:watch:*",
        "compile:extension": "tsc -p ./",
        "compile:views": "webpack --mode development",
        "watch:extension": "tsc -p ./",
        "watch:views": "webpack --mode development --watch",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "jest ./out/test/extension.spec.js",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "srcdiff": "git diff -- ':!package-lock.json'"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/jest": "^26.0.19",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.19.9",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.2",
        "@types/vscode": "^1.52.0",
        "@typescript-eslint/eslint-plugin": "^4.9.0",
        "@typescript-eslint/parser": "^4.9.0",
        "concurrently": "^6.0.0",
        "css-loader": "^5.1.3",
        "eslint": "^7.15.0",
        "glob": "^7.1.6",
        "jest": "^26.6.3",
        "mocha": "^8.1.3",
        "npm-run-all": "^4.1.5",
        "style-loader": "^2.0.0",
        "ts-loader": "^8.0.18",
        "typescript": "^4.1.2",
        "vscode-test": "^1.4.1",
        "webpack": "^5.27.0",
        "webpack-cli": "^4.5.0"
    },
    "dependencies": {
        "@svgdotjs/svg.js": "^3.0.16",
        "@types/styled-components": "^5.1.9",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-icons": "^4.2.0",
        "styled-components": "^5.2.1",
        "svgdom": "^0.1.8"
    }
}
